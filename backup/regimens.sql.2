INSERT INTO RegimenDetail(pk, SubCancerType2Id, name, schedule, emetogenic_potential, reference) VALUES (314,36,'<b>CODOX-M/IVAC</b>','<u>Cycles 1 AND 3 (CODOX-M)</u><br> <b>Cyclophosphamide</b> 800 mg/m<sup>2</sup> IV, day 1<br> <b>Cyclophosphamide</b> 600 mg/m<sup>2</sup>/d IV, days 2-5<br> <b>Doxoubicin</b> 40 mg/m<sup>2</sup> IV, day 1<br> <b>Vincristine<sup>b</sup></b> 1.5 mg/m<sup>2</sup> IV up to a maximum dose of 2 mg, days 1,8 for cycle 1, days 1,8, 15 for cycle 2<br> <b>MTX</b> 1,200 mg/m<sup>2</sup> IV over 1 hour, day 10,          <b>THEN</b> 240 mg/m<sup>2</sup>/h CIVI for the next 23 hours<br> <b>Leucovorin</b> calicium 192 mg/m<sup>2</sup> IT, day 11 at 36 hours <b>THEN</b> 12 mg/m<sup>2</sup> every 6 hours until MTX level is <5*10<sup>-8</sup> M<br> <b>Filgrastim 5 mcg/kg/d SQ until ANC >1,000/mm<sup>3</sup><br> <b>MTX</b> 12mg IT, day 15<br> <b>Leucovorin calicium</b> 15 mg PO 24 hours after MTX, day 16<br>                                                              <u>CNS PROPHYLAXIS</u><br> <b>Cytarabine</b> 70 mg IT, days 1,3,5<br> <b>MTX</b> 12 mg IT, day 12 Cycles 2 AND 4 (IVAC)<br> <b>Ifosfamide</b> 1,500mg/m<sup>2</sup> IV over 1 hour, days 1-5<br> <b>Mesna</b> 360 mg/m<sup>2</sup> IV over 1 hour <b>THEN</b> 360 mg/m<sup>2</sup> IV every 3 hours for a maximum of 7 doses  per 24 hour period, days 1-5<br> <b>Cytarabine</b>  2,000 mg/m<sup>2</sup> IV over 3 hours every 12 hours, days 1 and 2<br> <b>MTX</b> 12 mg IT, day 5<br> <b>Leucovorin calcium</b> 15 mg PO 24 hours after MTX, day 6<br> <b>Filgrastim</b> 5 mcg/kg/d SQ until ANC > 1,000/mm<sup>3</sup>                                                  ','Moderate','Mead GM, Sydes MR, Walewski J, et al. An international evaluation of CODOX-M and CODOX-M alternating with IVAC in adult Burkitt's lymphoma: results of United Kingdom Lymphoma Group LY06 Study. $Ann Oncol. 2002, 13(8): 1254-1274.$');
