INSERT INTO RegimenDetail(pk, SubCancerType2Id, name, schedule, emetogenic_potential, reference) VALUES (336,37,'<b>CODOX-M/IVAC</b>','<u>CYCLES 1 and 3 (CODOX-M)</u><br>  <b>Cyclophosphamide</b> 800 mg/m<sup>2</sup> IV, day 1<br> <b>Cyclophosphamide<b> 200 mg/m<sup>2</sup>/d IV, days 2-5<br>  <b>Doxorubicin</b> 40 mg/m<sup>2</sup> IV, day 1<br> <b>Vincristine<sup>b</sup></b> 1.5 mg/m<sup>2</sup> IV, days 1, 8 for cycle 1,days 1, 8, 15 for cycle 2<br> <b>MTX</b> 1,200 mg/m<sup>2</sup> IV over 1 h, day 10, <b>THEN</b> 240 mg/m2/h CIVI for the next 23 hours<br> <b>Leucovorin calicium</b> 50 mg IV q6h, begining 36 hours from the start of MTX until MTX level <0.05 mcM<br> <b>Filgrastim</b> beginning 24 h from the start of leucovorin until ANC>1,000<br> <u>CNS PROPHYLAXIS</u><br> <b>Cytarabine</b> 70 mg IT, days 1, 3<br> <b>MTX<b> 12 mg IT, day 15<br> <u>CYCLES 2 AND 4 (IVAC)</u><br> <b>Ifosofamide</b> 1,500 mg/m<sup>2</sup> IV, days 1-5<br> <b>Mesna</b> 1,500 mg/m<sup>2</sup> IV, days 1-5<br> <b>Etoposide</b> 60 mg/m<sup>2</sup> IV, days 1-5<br> <b>Cytarabine</b> 2,000 mg/m<sup>2</sup> IV q 12h, days 1 and 2<br> <b>Filgrastim</b> beginning 24 h after completion of chemotherapy until ANC >1,000<br> <u>CNS PROPHYLAXIS</u><br> <b>MTX</b> 12 mg IT, day 15<br> Additional IT Treatment for CNS disease- positive patients:<br> Cycle 1: cytarabine 70 mg IT, days 1,3,5, and MTX 12 mg IT, days 15,17<br> Cycle 3: cytarabine 70 mg IT, days 1,3, and MTX 12 mg IT, day 15                                                                                                                                                               ','Moderate','Wang ES, Strauss DJ, Teruya-Feldstein J, et al. Intensive chemotherapy with cyclophosphosphamide, doxorucibicin, high-dose methotrecate/ ifosfamide, etoposide, and high-dose cytarabine (CODOX-M/IVAC)for human immunodeficiency virus-associated Burkitt lymphoma. $Cancer. 2003;98(6): 1196-1205.$');
INSERT INTO RegimenDetail(pk, SubCancerType2Id, name, schedule, emetogenic_potential, reference) VALUES (337,37,'<b>Dose-adjusted EPOCH</b>','<b>Etoposide</b> 50 mg/m<sup>2</sup>/d CIVI, days 1-4 (96 hours)<br> <b>Doxrubicin</b> 10 mg/m<sup>2</sup>/d CIVI, days 1-4 (96 hours)<br> <b>Vincristine</b> 0.4 mg/m<sup>2</sup>/d /CIVI, days 1-4 (96 hours)<br> <b>Cyclophosphamide</b> Cycle1: 375 mg/m<sup>2</sup> IV, day 5, if CD4+ cells >= 100/mm<sup>3</sup>; 187 mg/m<sup>2</sup> IV, day 5, if CD4+ cells<100/mm<sup>3</sup><br> After cycle 1: increase dose by 187 mg if nadir ANC > 500/mvL or platelets <25,000/mcL<br> <b>Prednisone</b> 60 mg/m<sup>2</sup>/d PO, days 1-5<br> <b>Filgrastim</b> 5 mcg/kg/d SQ, day 6, and until ANC >5,000/mcl<br> Repeat every 21 days','Moderate','Little RF, Pittaluga S, Grant N, et al. Highly effective treatment of acquired immunodeficiency syndrome-related lymphoma with dose-adjusted EPOCH: impact of antiretroviral therapy suspension and tumor biology. $BLOOD.2003;101(12):4653-4659.$');
INSERT INTO RegimenDetail(pk, SubCancerType2Id, name, schedule, emetogenic_potential, reference) VALUES (338,38,'<b>CALGB 8811</b>','<u>INDUCTION (COURSE 1:4 WEEKS)</u><br> <b>Cyclophosphamide</b> 1,200 mg/m<sup>2</sup> IV, day1 (800 mg/m<sup>2</sup> for patients >=60 years)<br> <b>Daunorubicin</b> 45 mg/m<sup>2</sup> IV, days 1-3 (30 mg/m<sup>2</sup> for patients>=60 years)<br> <b>Vincristine</b> 2 mg IV, days 1, 8, 15, 22<br> <b>Prednisone</b> 60 mg/m<sup>2</sup>/d PO, days 1-21 (days 1-7 for patients >=60 years)<br> <b>Asparaginase</b> 6,000 units/m<sup>2</sup> SQ, days 5, 8, 11, 15, 18, 22<br> <u>EARLY INTENSIFICATION (COURSE 2:4 WEEKS, REPEAT ONCE)</u><br> <b>MTX</b> 15 mg IT, day 1<br> <b>Cyclophosphamide</b> 1,000 mg/m<sup>2</sup> IV, day 1<br> <b>Mercaptopurine</b> 60 mg/m<sup>2</sup>/d PO, days 1-14<br> <b>Cytarabine</b> 75 mg/m<sup>2</sup>/d SQ, days 1-4, 8-11<br> <b>Vincristine</b> 2 mg IV, days 15, 22<br> <b>Asparaginase</b> 6,000 units/m<sup>2</sup> SQ, days 15, 18, 22, 25<br> <u>CNS PROPHYLAXIS AND INTERIM MAINTENANCE (COURSE 3:12 WEEKS)</u><br> <b>Cranial RT</b> 2,400 cGy, days 1-12<br> <b>MTX</b> 15 mg IT, days 1, 8, 15, 22, 29<br> <b>Mercaptopurine</b> 60 mg/m<sup>2</sup>/d PO, days 1-70<br> <b>MTX</b> 20 mg/m<sup>2</sup> PO, days 36, 43, 50, 57, 64<br> <u>LATE INTENSIFICATION (COURSE 4:8 WEEKS)</u><br> <b>Doxorubicin</b> 30 mg/m<sup>2</sup> IV, days 1, 8, 15<br> <b>Vincristine</b> 2 mg IV, days 1, 8, 15<br> <b>Dexamethasone</b> 10 mg/m<sup>2</sup>/d PO, days 1-14<br> <b>Cyclophosphamide</b> 1,000 mg/m<sup>2</sup> IV, day 29<br> <b>Thioguanine</b> 60 mg/m<sup>2</sup>/d PO, days 29-42<br> <b>Cytarabine</b> 75 mg/m<sup>2</sup>/d SQ, days 29-32, 36-39<br> <u>MAINTENANCE (COURSE 5:4 WEEKS, FOR 2 YEARS FROM DIAGNOSIS)</u><br> <b>Vincristine</b> 2 mg IV, day 1 of every 4 weeks<br> <b>Prednisone</b> 60 mg/m<sup>2</sup>/d PO, days 1-5 of every 4 weeks<br> <b>MTX</b> 20 mg/m<sup>2</sup> PO, days 1, 8, 15, 22<br> <b>Mercaptopurine</b> 60 mg/m<sup>2</sup>/d PO